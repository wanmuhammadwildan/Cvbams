{
    "version": 2,
    "framework": null,
    "functions": {
        "public/index.php": {
            "runtime": "vercel-php@0.6.1"
        }
    },
    "rewrites": [
        {
            "source": "/build/(.*)",
            "destination": "/public/build/$1"
        },
        {
            "source": "/css/(.*)",
            "destination": "/public/css/$1"
        },
        {
            "source": "/js/(.*)",
            "destination": "/public/js/$1"
        },
        {
            "source": "/images/(.*)",
            "destination": "/public/images/$1"
        },
        {
            "source": "/favicon.ico",
            "destination": "/public/favicon.ico"
        },
        {
            "source": "/robots.txt",
            "destination": "/public/robots.txt"
        },
        {
            "source": "/(.*)",
            "destination": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:0BQFXcVGHfSQ4smOkWA40oqLoa5AGpby5bbOGwc7xFo=",
        "APP_URL": "https://cvbams.vercel.app",
        "ASSET_URL": "https://cvbams.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "pgsql"
    }
}